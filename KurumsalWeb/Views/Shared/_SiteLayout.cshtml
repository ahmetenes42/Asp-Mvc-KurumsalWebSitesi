                            <li><i class="bx bx-chevron-right"></i> <a href="#">Blog</a></li>
                            <li><i class="bx bx-chevron-right"></i> <a href="/Home/#iletisim">İletişim</a></li>
                        </ul>
                    </div>


                    <div class="col-lg-4 col-md-6 footer-contact">
                        <h4>Adres</h4>
                        <p>
                            @ViewBag.Iletisim.Adres
                            <br />
                            <strong>Telefon:</strong>@ViewBag.Iletisim.Tel <br>
                            <strong>Fax:</strong>@ViewBag.Iletisim.Fax <br>
                        </p>

                    </div>

                    <div style="position:relative;top:200px; left:250px;" class="col-lg-3 col-md-6 footer-info">
                        <div class="social-links mt-3">
                            <a href="@ViewBag.Iletisim.Twitter" target="_blank" class="twitter"><i class="bx bxl-twitter"></i></a>
                            <a href="@ViewBag.Iletisim.Facebook" target="_blank" class="facebook"><i class="bx bxl-facebook"></i></a>
                            <a href="@ViewBag.Iletisim.Instagram" target="_blank" class="instagram"><i class="bx bxl-instagram"></i></a>
                            <a href="@ViewBag.Iletisim.Whatsapp" target="_blank" class="linkedin"><i class="bx bxl-whatsapp"></i></a>
                        </div>
                    </div>

                </div>
