        <div class="container d-flex justify-content-between align-items-center">

            <div class="logo">
                <h1 class="text-light"><a href="/Home/Index"><span>Kurumsal Web Sitesi</span></a></h1>
                <!-- Uncomment below if you prefer to use an image logo -->
                <!-- <a href="index.html"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->
            </div>

            <nav id="navbar" class="navbar">
                <ul>
                    <li><a class="" href="/Home/Index">Ana Sayfa</a></li>
                    <li><a href="/Home/Hakkimizda">Hakkımızda</a></li>
                    <li><a href="/Home/Hizmetlerimiz">Hizmetlerimiz</a></li>
                    <li><a href="/Home/Blog">Blog</a></li>
                    <li><a class="active" href="/Home/Iletisim">İletişim</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->

        </div>
    </header><!-- End Header -->

    @RenderBody()
    <main id="main">





        </div>
        </section><!-- End Services Section -->




    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer" data-aos="fade-up" data-aos-easing="ease-in-out" data-aos-duration="500">


        <div class="footer-top">
            <div class="container">
                <div class="row">

                    <div class="col-lg-4 col-md-5 footer-links">
                        <h4>Menü</h4>
                        <ul>
                            <li><i class="bx bx-chevron-right"></i> <a href="/Home/#index">Ana Sayfa</a></li>
                            <li><i class="bx bx-chevron-right"></i> <a href="/Home/#Hakkimizda">Hakkımızda</a></li>
